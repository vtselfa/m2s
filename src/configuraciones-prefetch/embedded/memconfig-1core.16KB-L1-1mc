Channels = 2
CyclesSendRequest = 1
CyclesRowBufferHit = 33
CyclesRowBufferMiss = 100
Bandwith = 16
CyclesProcByCyclesBus = 3
Threshold= 100000
PolicyMCQueues = PrefetchNormalQueue
