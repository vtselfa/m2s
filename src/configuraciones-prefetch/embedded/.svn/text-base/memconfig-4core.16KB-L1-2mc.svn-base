[ CacheGeometry l1 ]
Sets = 128
Assoc = 2
BlockSize = 64
Latency = 1
; Prefetch = 0

[ CacheGeometry l2 ]
Sets = 512
Assoc = 16
BlockSize = 64
Latency = 10
; Prefetch = 1

#
# Level 1 Caches
#
[ Module dl1-0 ]
Type = Cache
Geometry = l1
LowNetwork = net-0
LowModules = l2-0

[ Module il1-0 ]
Type = Cache
Geometry = l1
LowNetwork = net-0
LowModules = l2-0

[ Module dl1-1 ]
Type = Cache
Geometry = l1
LowNetwork = net-1
LowModules = l2-1

[ Module il1-1 ]
Type = Cache
Geometry = l1
LowNetwork = net-1
LowModules = l2-1

[ Module dl1-2 ]
Type = Cache
Geometry = l1
LowNetwork = net-2
LowModules = l2-2

[ Module il1-2 ]
Type = Cache
Geometry = l1
LowNetwork = net-2
LowModules = l2-2

[ Module dl1-3 ]
Type = Cache
Geometry = l1
LowNetwork = net-3
LowModules = l2-3

[ Module il1-3 ]
Type = Cache
Geometry = l1
LowNetwork = net-3
LowModules = l2-3

#
# Level 2 Caches
#
[ Module l2-0 ]
Type = Cache
Geometry = l2
HighNetwork = net-0
LowNetwork = mesh
LowNetworkNode = n0
LowModules = mod-mm-1 mod-mm-2

[ Module l2-1 ]
Type = Cache
Geometry = l2
HighNetwork = net-1
LowNetwork = mesh
LowNetworkNode = n1
LowModules = mod-mm-1 mod-mm-2

[ Module l2-2 ]
Type = Cache
Geometry = l2
HighNetwork = net-2
LowNetwork = mesh
LowNetworkNode = n2
LowModules = mod-mm-1 mod-mm-2

[ Module l2-3 ]
Type = Cache
Geometry = l2
HighNetwork = net-3
LowNetwork = mesh
LowNetworkNode = n3
LowModules = mod-mm-1 mod-mm-2

#
# Main memory
#
[ Module mod-mm-1 ]
Type = MainMemory
HighNetwork = mesh
HighNetworkNode = n4
Latency = 200
BlockSize = 64
AddressRange = ADDR DIV 1024 MOD 2 EQ 0

[ Module mod-mm-2 ]
Type = MainMemory
HighNetwork = mesh
HighNetworkNode = n5
Latency = 200
BlockSize = 64
AddressRange = ADDR DIV 1024 MOD 2 EQ 1

#
# Nodes
#
[ Entry core-0 ]
Type = CPU
Core = 0
Thread = 0
InstModule = il1-0
DataModule = dl1-0

[ Entry core-1 ]
Type = CPU
Core = 1
Thread = 0
InstModule = il1-1
DataModule = dl1-1

[ Entry core-2 ]
Type = CPU
Core = 2
Thread = 0
InstModule = il1-2
DataModule = dl1-2

[ Entry core-3 ]
Type = CPU
Core = 3
Thread = 0
InstModule = il1-3
DataModule = dl1-3

#
# Interconnects
#
[ Network net-0 ]
DefaultBandWidth = 128
DefaultInputBufferSize = 512
DefaultOutputBufferSize = 512

[ Network net-1 ]
DefaultBandWidth = 128
DefaultInputBufferSize = 512
DefaultOutputBufferSize = 512

[ Network net-2 ]
DefaultBandWidth = 128
DefaultInputBufferSize = 512
DefaultOutputBufferSize = 512

[ Network net-3 ]
DefaultBandWidth = 128
DefaultInputBufferSize = 512
DefaultOutputBufferSize = 512

